{#
/**
 * @file
 * Theme override for the Content Insights dashboard.
 */
#}
<div {{ attributes.addClass('content-insights-dashboard') }}>
  <div class="insight-item">
    <strong>Word Count:</strong>
    <span class="insight-value" data-insight="word_count">0</span>
  </div>
  <div class="insight-item">
    <strong>Reading Time:</strong>
    <span class="insight-value" data-insight="reading_time">0 mins</span>
  </div>
</div>
